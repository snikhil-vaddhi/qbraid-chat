{
  "name": "qbraid-chat",
  "displayName": "qbraid-chat",
  "description": "A chat interface for interacting with qBraid's REST API",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/snikhil-vaddhi/qbraid-chat.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:qbraid-chat.start",
    "onCommand:qbraid-chat.clearApiKey"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "authentication": [
      {
        "label": "Api-Key",
        "id": "key"
      }
    ],
    "commands": [
      {
        "command": "qbraid-chat.start",
        "title": "qbraid chat start"
      },
      {
        "command": "qbraid-chat.clearApiKey",
        "title": "Clear qBraid API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsce:package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "node-fetch": "^3.3.2"
  },
  "vsce": {
    "packageFiles": [
      "qbraid-chat-0.1.0.vsix"
    ]
  }
}
